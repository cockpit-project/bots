#!/bin/bash
set -ex

URL=http://download.devel.redhat.com/rhel-10/nightly/RHEL-10/latest-RHEL-10.1/compose/BaseOS/x86_64/images/
IMAGE=$(curl -L -s "$URL" | sed -n '/<a href=.*rhel-guest-image.*qcow2"/ { s/^.*href="//; s_".*$__; p }')
[ -n "$IMAGE" ]

exec $(dirname $0)/lib/cloudimage.bootstrap "$1" "$URL/$IMAGE"
